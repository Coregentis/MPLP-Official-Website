    "@next/bundle-analyzer": "^16.1.0",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "js-yaml": "^4.1.1",
    "pagefind": "^1.4.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
