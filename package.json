    "render:jump-map": "node scripts/governance/render-jump-map.mjs",
    "generate:seo-manifest": "node scripts/governance/generate-seo-manifest.mjs",
    "verify:semantic-tier": "WG05_STRICT=1 node scripts/governance/semantic-tiering.mjs",
